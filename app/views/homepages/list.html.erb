<h1 class="list">Recipes</h1>
<section>
<div class="row small-up-1 medium-up-3">
<% @data.each do |data|%>

<!-- <div class="small-2 medium-3 large-4 columns"> -->
<div class="columns"><p><%= data.label %></p>
<%= image_tag data.image, size: "120x120"%>
<%= link_to("Recipe", show_path(data.uri)) %>
</div>
<%end%>
</div>
<div id="page">
<p><%= paginate @data%></p>
</div>
</section>
